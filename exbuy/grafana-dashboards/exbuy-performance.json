{
  "dashboard": {
    "title": "ExBuy Performance Dashboard",
    "tags": ["exbuy", "django", "performance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Requests Per Second (RPS)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(django_http_requests_total_by_method_total[1m])) by (method)",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Response Time (p95, p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(django_http_requests_latency_seconds_bucket[1m])) by (le, view))",
            "legendFormat": "p95 - {{view}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(django_http_requests_latency_seconds_bucket[1m])) by (le, view))",
            "legendFormat": "p99 - {{view}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(django_http_responses_total_by_status_total{status=~\"5..\"}[1m])) / sum(rate(django_http_requests_total_by_method_total[1m])) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error %", "max": 100, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Database Query Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(django_db_query_duration_seconds_sum[1m]) / rate(django_db_query_duration_seconds_count[1m])",
            "legendFormat": "Avg Query Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(django_db_query_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "p95 Query Time",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "DB Queries Per Second",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(django_db_query_duration_seconds_count[1m]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "qps",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Active Requests",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "django_http_requests_inprogress_total",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Request Status Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum by (status) (django_http_responses_total_by_status_total)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Top Endpoints by Request Count",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "topk(10, sum by (view) (rate(django_http_requests_total_by_view_transport_method_total[5m])))",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "view": "Endpoint",
                "Value": "Requests/sec"
              }
            }
          }
        ]
      }
    ]
  },
  "overwrite": true
}
